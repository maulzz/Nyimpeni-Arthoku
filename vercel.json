{
    "version": 2,
    "builds": [
        {
            "src": "public/index.php",
            "use": "vercel-php@0.6.1"
        }
    ],
    "routes": [
        { "src": "/build/(.*)", "dest": "/public/build/$1" },
        { "src": "/storage/(.*)", "dest": "/storage/$1" },
        { "src": "/(favicon.ico|logo.svg|robots.txt)", "dest": "/public/$1" },
        { "handle": "filesystem" },
        { "src": "/(.*)", "dest": "/public/index.php" }
    ]
}